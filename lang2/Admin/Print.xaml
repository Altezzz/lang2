<Window x:Class="lang2.Admin.Print"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lang2.Admin"
        mc:Ignorable="d"
        Title="Отчеты" 
        Background="#808080"
        Height="300" 
        Width="30"
        MaxHeight="400"
        MaxWidth="450"
        MinHeight="300"
        MinWidth="360"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Icon="Library.ico">
    <Grid>
        <Label Content="Отчеты"
               Grid.Row="0"
               HorizontalAlignment="Center"
               VerticalAlignment="Center"
               Margin="134,38,134,193"
               Height="40"
               FontSize="22"
               FontWeight="Medium"/>
        <Button x:Name="ex1" HorizontalAlignment="Left" Content="Excel№1"  Margin="120,106,0,117" Style="{DynamicResource ButtonStyle1}" Click="ex1_click"  Width="117"/>
        <Button x:Name="ex2" HorizontalAlignment="Left" Content="Excel№2"  Margin="120,186,0,37" Style="{DynamicResource ButtonStyle1}" Click="ex2_click"  Width="117"/>


    </Grid>
</Window>
